{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "outside-app",
  "version": "0.1.0",
  "identifier": "com.dingdangmaoup.outside.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "outside-app",
        "minWidth": 1024,
        "minHeight": 500,
        "width": 1200,
        "height": 900,
        "decorations": false,
        "shadow":true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
